{
  //consul
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Scheme": "http",
      "Host": "localhost",
      "Port": 8500,
      "Type": "Consul"
    }
  },
  "Routes": [
    {
      "UpstreamPathTemplate": "/WebApplication1{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Put",
        "Post",
        "Delete"
      ],
      "DownstreamScheme": "http",
      "DownstreamPathTemplate": "/WebApplication1/{everything}",
      "ServiceName": "test",
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    }
  ]
}