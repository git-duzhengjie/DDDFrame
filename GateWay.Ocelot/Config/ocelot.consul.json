{
  //consul
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Scheme": "http",
      "Host": "localhost",
      "Port": 8500,
      "Type": "Consul"
    }
  },
  "Routes": [
    {
      "UpstreamPathTemplate": "/LetUsMarry{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Put",
        "Post",
        "Delete"
      ],
      "DownstreamScheme": "http",
      "DownstreamPathTemplate": "/LetUsMarry/{everything}",
      "ServiceName": "LetUsMarry",
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    },
    {
      "UpstreamPathTemplate": "/Cloud{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Put",
        "Post",
        "Delete"
      ],
      "DownstreamScheme": "http",
      "DownstreamPathTemplate": "/Cloud/{everything}",
      "ServiceName": "Cloud",
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    },
    {
      "UpstreamPathTemplate": "/File{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Put",
        "Post",
        "Delete"
      ],
      "DownstreamScheme": "http",
      "DownstreamPathTemplate": "/File/{everything}",
      "ServiceName": "file",
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    }
  ]
}